{
  "name": "home",
  "private": true,
  "version": "1.0.0",
  "description": "Personal homepage of Chris Mahlke, Application Developer",
  "author": "Chris Mahlke <cmahlke@gmail.com>",
  "license": "MIT",
  "type": "module",
  "homepage": "https://chrismahlke.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/ChrisMahlke/home.git"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "scripts": {
    "dev": "node scripts/generate-html.js && vite",
    "build": "node scripts/generate-html.js && node scripts/generate-manifest.js && vite build",
    "lint": "eslint .",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "preview": "vite preview",
    "clean": "rm -rf dist node_modules",
    "generate-manifest": "node scripts/generate-manifest.js",
    "generate-html": "node scripts/generate-html.js"
  },
  "dependencies": {
    "lucide-react": "^0.469.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@vitejs/plugin-react": "^4.3.4",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.17.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-promise": "^7.2.1",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-sonarjs": "^3.0.1",
    "eslint-plugin-unicorn": "^56.0.1",
    "globals": "^15.13.0",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.17",
    "terser": "^5.37.0",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.19.1",
    "vite": "^5.4.11"
  }
}
